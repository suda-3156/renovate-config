{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": ["/^biome\\.json$/"],
      "matchStrings": [
        "https://biomejs\\.dev/schemas/(?<currentValue>[^/]+)/schema\\.json"
      ],
      "datasourceTemplate": "npm",
      "depNameTemplate": "@biomejs/biome"
    }
  ]
}
